{
"manifest_version": 3,
"name": "kifixer",
"version": "1.0",
"description": "Remove third-party and sponsored content advertisements from Kijiji.ca",
"homepage_url": "http://github.com/snacsnoc/kifixer",
"permissions": [
    ],
"content_scripts": [
      {
            "matches": ["https://*.kijiji.ca/*"],
            "js": ["js/main.js"],
            "run_at": "document_end" 
      }
],
"icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128":"images/icon.png"
   }
}